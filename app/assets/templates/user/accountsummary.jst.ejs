<div class="container">
  <h1>Account Summary</h1>
  <div class="summary-list col-md-6 col-md-offset-3 col-xs-6 col-xs-offset-3">
    <p class="name">
      Name: <span><%= FitsApp.SessionModel.get('fname') + " " + FitsApp.SessionModel.get('lname') %></span>
    </p>
    <p class="email">
      Email: <span><%= FitsApp.SessionModel.get('email') %></span>
    </p>
  </div>
</div>
